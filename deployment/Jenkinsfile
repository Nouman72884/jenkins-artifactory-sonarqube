pipeline {
    environment {
        BUILD_NUM = ''
    }
    agent any
    parameters {
    string(name: 'Build_Number', defaultValue: '2', description: 'Build number')

 }
    stages {
     stage ('download artifact') {
        steps{
            sh 'curl -O   http://artifactory.eurustechnologies.info/artifactory/maven_local/my-app-1.0-SNAPSHOT.${params.Build_Number}.jar'
        }
    }
   
  }
}
  